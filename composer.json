{
    "name": "jairocgr/datashot",
    "description": "A tool for taking partial database dumps for testing and development",
    "type": "library",
    "license": "MIT",
    "version": "0.1.0",
    "homepage": "https://github.com/jairocgr/datashot",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Jairo Rodrigues Filho",
            "email": "jairocgr@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": { "": "src/" }
    },
    "require": {
        "php": ">=5.6",
        "ext-pdo": "*",
        "ext-zlib": "*",
        "symfony/console": "~3.3",
        "symfony/process": "^4.2",
        "aws/aws-sdk-php": "^2.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "vlucas/phpdotenv": "^2.5"
    },
    "scripts": {
        "post-install-cmd": [
            "[ -f .env ] || cp .env.example .env"
        ]
    },
    "bin": ["bin/datashot"]
}
